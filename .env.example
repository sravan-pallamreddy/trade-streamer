# Copy to .env and fill in (no secrets required for quotes)

# Options recommender defaults (override at runtime if desired)
# DEFAULT_IV=0.2
# RISK_FREE=0.01
# RISK_PCT=0.01
# OTM_PCT=0.02
# MIN_BUSINESS_DAYS=2
# STOP_LOSS_PCT=0.5
# TAKE_PROFIT_MULT=2.0
# STREAM_INTERVAL_SEC=30

# Required for streaming suggestions unless provided via --account
# ACCOUNT_SIZE=25000

# E*TRADE (for equities/options data)
# ETRADE_BASE_URL=https://apisb.etrade.com
# ETRADE_CONSUMER_KEY=
# ETRADE_CONSUMER_SECRET=
# ETRADE_CALLBACK=oob
# ETRADE_ACCESS_TOKEN=
# ETRADE_ACCESS_TOKEN_SECRET=

# AI provider (optional AI enrichment)
# AI_PROVIDER=openai
# UI_AI_PROVIDERS=openai,xai  # Optional: override auto-detection for dashboard scans
# OPENAI_API_KEY=
# OPENAI_BASE_URL=https://api.openai.com/v1
# XAI_API_KEY=
# XAI_BASE_URL=https://api.x.ai/v1
# USE_AI=true
# AI_MODEL=gpt-4o-mini
# AI_INTERVAL_SEC=60

# Output controls
# TABLE_OUTPUT=true
# JSON_OUTPUT=true

# Rule gates (optional automation)
# RULES=true
# RULES_STRICT=true
# RULES_FAST=5
# RULES_SLOW=20
# RULES_FUTURES=true
# RULES_VWAP=true
# RULES_RVOL=1.5

# Optional defaults for options recommender
# DEFAULT_IV=0.2
# RISK_FREE=0.01
# Options selection (live chain)
# OPTIONS_EXPIRY=YYYY-MM-DD
# TARGET_DELTA=0.3
# TARGET_PREMIUM=0.2
